<?xml version="1.0" encoding="UTF-8" ?>

<phpunit
        backupGlobals               = "false"
        backupStaticAttributes      = "false"
        colors                      = "false"
        convertErrorsToExceptions   = "false"
        convertNoticesToExceptions  = "false"
        convertWarningsToExceptions = "false"
        processIsolation            = "false"
        stopOnFailure               = "false"
